    "outDir": "./build/js/",
    "target": "es5",
    "module": "es2015",
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "moduleResolution": "node",
    "jsx": "react"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}